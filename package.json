{
    "name": "@wxn0brp/db-plugin-dir",
    "version": "0.0.1",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "author": "wxn0brP",
    "license": "MIT",
    "type": "module",
    "description": "ValtheraDB Dir Plugin",
    "repository": {
        "type": "git",
        "url": "https://github.com/wxn0brP/ValtheraDB-plugin-dir.git"
    },
    "homepage": "https://github.com/wxn0brP/ValtheraDB",
    "scripts": {
        "build": "tsc && tsc-alias"
    },
    "dependencies": {
        "json5": "^2.2.3",
        "@wxn0brp/db-core": ">=0.0.1 <0.1.0"
    },
    "devDependencies": {
        "@types/node": "^22.12.0",
        "tsc-alias": "^1.8.10",
        "typescript": "^5.7.3"
    },
    "files": [
        "dist"
    ],
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "default": "./dist/index.js"
        },
        "./*": {
            "types": "./dist/*.d.ts",
            "import": "./dist/*.js",
            "default": "./dist/*.js"
        }
    }
